{
  "name": "kannywood-names",
  "version": "1.0.0",
  "description": "Get random Star Wars names",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "babel --copy-files --out-dir dist --ignore *.test.js src",
    "test": "nyc mocha",
    "watch:test": "mocha --watch",
    "lint": "eslint src",
    "validate": "npm-run-all --parallel test lint build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zenitchoder/kannywood-names.git"
  },
  "keywords": [
    "random",
    "star wars"
  ],
  "files": [
    "dist"
  ],
  "author": "Awonusi Olajide <awonsiolajide@yahoo.com> (http://zenithcoder.github.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zenitchoder/kannywood-names/issues"
  },
  "homepage": "https://github.com/zenitchoder/kannywood-names#readme",
  "dependencies": {
    "unique-random-array": "1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-preset-es2015": "6.9.0",
    "chai": "3.5.0",
    "eslint": "3.2.0",
    "eslint-config-kentcdodds": "^9.0.0",
    "ghooks": "1.3.2",
    "mocha": "3.0.0",
    "npm-run-all": "2.3.0",
    "nyc": "7.1.0",
    "rimraf": "2.5.4"
  },
  "nyc": {
    "all": true,
    "check-coverage": true,
    "branches": 100,
    "function": 100,
    "lines": 100,
    "statements": 100,
    "reporter": [
      "text",
      "lcov"
    ],
    "include": [
      "src"
    ]
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run validate"
    }
  }
}
